include Makefile.inc

C_SRCS += \
src/devicehandler.c \
src/liewebgw.c \
src/settings.c \
src/signals.c \
src/log.c

OBJS += \
src/devicehandler.o \
src/liewebgw.o \
src/settings.o \
src/signals.o \
src/log.o

C_DEPS += \
src/devicehandler.d \
src/liewebgw.d \
src/settings.d \
src/signals.d \
src/log.d

# Each subdirectory must supply rules for building sources it contributes
%.o: %.c
	@echo 'Building file: $<'
	@echo 'Invoking: GCC C Compiler'
	$(CC) $(CFLAGS) -c  -o $@ $<
	@echo 'Finished building: $<'
	@echo ' '  
	

